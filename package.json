{
  "name": "karaokecapital",
  "version": "1.0.0",
  "description": "All purpose karaoke app",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run bulid --prefix client ",
    "install": "npm install --prefix client",
    "build": "npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PlainOlFred/KaraokeCapital.git"
  },
  "author": "Jeremy Jones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PlainOlFred/KaraokeCapital/issues"
  },
  "homepage": "https://github.com/PlainOlFred/KaraokeCapital#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
